{
  "name": "hsync-server",
  "version": "0.5.0",
  "description": "creates unlimited public web URLs for your web servers no matter where they may be running",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./deps": "./deps.js"
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.js\" --ignore-path .gitignore",
    "dev": "nodemon --inspect run.js",
    "dev-brk": "nodemon --inspect-brk run.js",
    "start": "node run.js",
    "build": "webpack"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/cookie": "^12.0.1",
    "@hapi/hapi": "^21.4.4",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "aedes": "^0.51.3",
    "b64id": "^1.1.1",
    "debug": "^4.4.3",
    "handlebars": "^4.7.8",
    "hapi-swagger": "^17.3.2",
    "joi": "^17.13.3",
    "lodash.sample": "^4.2.1",
    "rawr": "^0.19.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.0.0",
    "htm": "^3.1.1",
    "nodemon": "^3.1.11",
    "preact": "^10.28.2",
    "prettier": "^3.8.0",
    "vitest": "^4.0.17",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  }
}
